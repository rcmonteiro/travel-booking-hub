    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.12.8",
    "tsup": "^8.0.2",
    "tsx": "^4.9.0"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@prisma/client": "5.13.0",
    "env": "workspace:*",
    "bcryptjs": "^2.4.3",
    "dotenv-cli": "^7.4.1",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.1.9",
