{
  "name": "user-service",
  "version": "0.0.1",
  "private": true,
  "main": "src/http/server.ts",
  "scripts": {
    "start:dev": "pnpm env:load tsx watch src/http/server.ts",
    "start:prod": "node build/server.js",
    "build": "tsup src --out-dir build",
