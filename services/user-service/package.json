{
  "name": "@tbh/user-service",
  "version": "0.0.1",
  "private": true,
  "main": "src/infra/server.ts",
  "scripts": {
    "start:dev": "pnpm env:load tsx watch src/infra/server.ts",
    "start:prod": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest",
    "test:e2e": "pnpm env:load vitest run --dir src/infra",
    "test:e2e:watch": "pnpm env:load vitest --dir src/infra",
    "pretest:e2e": "cd prisma/vitest-environment-prisma && pnpm link ./ && cd ../.. && pnpm link ./prisma/vitest-environment-prisma",
    "db:migrate": "pnpm env:load prisma migrate dev",
    "db:generate": "pnpm env:load prisma generate",
    "db:studio": "pnpm env:load prisma studio",
    "db:seed": "pnpm env:load prisma db seed",
    "env:load": "dotenv -e ../../.env --"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/node": "^20.12.8",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.5",
    "@tbh/eslint-config": "workspace:*",
    "@tbh/prettier": "workspace:*",
    "supertest": "^7.0.0",
    "@tbh/tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "tsx": "^4.9.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@prisma/client": "5.13.0",
    "@tbh/core": "workspace:*",
    "dotenv-cli": "^7.4.1",
    "@tbh/env": "workspace:*",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.1.9",
    "@tbh/message-broker": "workspace:*",
    "prisma": "^5.14.0",
    "zod": "^3.23.8"
  },
  "prettier": "@tbh/prettier",
  "eslintConfig": {
    "extends": [
      "@tbh/eslint-config/node"
    ]
  },
  "packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195"
}
